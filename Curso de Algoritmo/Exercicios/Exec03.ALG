algoritmo "semnome"
var
      nome:caracter
      salario:real
inicio
       Escreva("Qual o nome do funcionário? ")
       Leia(nome)
       Escreva("Qual o salário do funcionário? ")
       Leia(salario)
       Escreval("Nome do Funcionário: ",nome)
       Escreval("Salário: ",salario)
       Escreva("O funcionário ",nome," tem um salário de R$",salario," em Junho.")
fimalgoritmo