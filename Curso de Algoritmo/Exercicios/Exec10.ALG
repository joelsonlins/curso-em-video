algoritmo "semnome"
var    area,altura,largura,tinta:real

inicio
        Escreva("Digite a altura da parede: ")
        Leia(altura)
        Escreva("Digite a largura da parede: ")
        Leia(largura)
        area <- altura * largura
        tinta <-  area/2
        Escreva("Em ",area,"m² usaremos ",tinta," Litros de tinta.")

fimalgoritmo