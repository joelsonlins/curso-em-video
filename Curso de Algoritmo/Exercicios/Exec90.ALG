algoritmo "semnome"
var
num1,num2:inteiro
procedimento Somador(a,b:inteiro)
inicio
        Escreva("Resultado da soma: ",a+b)
fimprocedimento
inicio
    Escreva("Digite o primeiro valor: ")
    Leia(num1)
    Escreva("Digite o segundo valor: ")
    Leia(num2)
    
    Somador(num1,num2)
fimalgoritmo