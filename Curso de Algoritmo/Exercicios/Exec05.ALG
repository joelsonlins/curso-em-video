algoritmo "semnome"
var   nota01, nota02, media:real

inicio
      Escreva("Digite a primeira nota: ")
      Leia(nota01)
      Escreva("Digite a segunda nota: ")
      Leia(Nota02)
      media <- (nota01 + nota02)/2
      Escreval("Nota 1: ",nota01)
      Escreval("Nota 2: ",nota02)
      Escreva("A média entre",nota01," e ",nota02," é igual a ",media)

fimalgoritmo