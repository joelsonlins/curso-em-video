algoritmo "semnome"
var
V: vetor[1..6] de Inteiro
c: Inteiro
inicio
          Para c <- 1 ate 6 faca
               Escreva("Digite o ", c, "o. valor: ")
               Leia(V[c])
          FimPara
          
          Para c <- 1 ate 6 faca
               Escreva("[", V[c], "]")
          FimPara
fimalgoritmo